{
    "terminal.integrated.env.windows": {
        "MSYSTEM": "MINGW64",
        "MSYS2_PATH_TYPE": "inherit",
        "CHERE_INVOKING": "enabled_from_arguments"
    },
    "terminal.integrated.shell.windows": "C:/msys64/usr/bin/bash.exe",
    "terminal.integrated.shellArgs.windows": [
        "--login"
    ],
    "files.associations": {
        "functional": "cpp",
        "*.tcc": "cpp",
        "system_error": "cpp",
        "array": "cpp",
        "compare": "cpp",
        "ranges": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp"
    },
}